<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ll_round_white"
        android:padding="16dp"
        android:layout_margin="16dp"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/promissory"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:gravity="center_vertical"
            android:text="@string/promissory"
            android:textColor="@color/color_1A1A1A"
            android:clickable="true"
            tools:ignore="HardcodedText" />

        <LinearLayout
            android:id="@+id/divideLine01"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/color_DBDCDF"
            android:layout_marginHorizontal="2dp"
            android:orientation="horizontal" />

        <TextView
            android:id="@+id/listed"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:gravity="center_vertical"
            android:text="@string/listed"
            android:textColor="@color/color_1A1A1A"
            android:clickable="true"
            tools:ignore="HardcodedText" />

        <LinearLayout
            android:id="@+id/divideLine02"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/color_DBDCDF"
            android:layout_marginHorizontal="2dp"
            android:orientation="horizontal" />

        <TextView
            android:id="@+id/householdCheck"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:gravity="center_vertical"
            android:text="@string/householdCheck"
            android:textColor="@color/color_1A1A1A"
            android:clickable="true"
            tools:ignore="HardcodedText" />

        <LinearLayout
            android:id="@+id/divideLine03"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/color_DBDCDF"
            android:layout_marginHorizontal="2dp"
            android:orientation="horizontal" />

        <TextView
            android:id="@+id/currentCheck"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:gravity="center_vertical"
            android:text="@string/currentCheck"
            android:textColor="@color/color_1A1A1A"
            android:clickable="true"
            tools:ignore="HardcodedText" />
    </LinearLayout>

</LinearLayout>